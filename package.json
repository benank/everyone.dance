{
    "name": "everyone.dance",
    "version": "1.1.6",
    "description": "everyone.dance",
    "main": "main.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/benank/everyone.dance.git"
    },
    "keywords": [],
    "author": "benank",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/benank/everyone.dance/issues"
    },
    "homepage": "https://github.com/benank/everyone.dance#readme",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "make-mac": "electron-forge make --platform darwin",
        "make-linux": "electron-forge make --platform linux --target zip",
        "webapp": "node webapp.js",
        "server": "node server/server.js",
        "server-test": "node server/server.js --test",
        "watch": "webpack --config webpack.common.js --watch",
        "dev": "npm-run-all -p -r start watch",
        "bundle": "webpack bundle --config webpack.common.js --mode production",
        "deploy": "npm run bundle && npm run webapp"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "electron-squirrel-startup": "^1.0.0",
        "express": "^4.17.1",
        "follow-redirects": "^1.13.2",
        "fs-extra": "^9.1.0",
        "gh-latest-release": "^1.0.0",
        "react": "^17.0.1",
        "react-countup": "^4.3.3",
        "react-dom": "^17.0.1",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "unzipper": "^0.10.11"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.12.10",
        "@electron-forge/cli": "^6.0.0-beta.54",
        "@electron-forge/maker-deb": "^6.0.0-beta.54",
        "@electron-forge/maker-rpm": "^6.0.0-beta.54",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
        "@electron-forge/maker-zip": "^6.0.0-beta.54",
        "babel-loader": "^8.2.2",
        "css-loader": "^5.0.1",
        "electron": "^11.2.1",
        "electron-reload": "^1.5.0",
        "npm-run-all": "^4.1.5",
        "null-loader": "^4.0.1",
        "sass": "^1.32.5",
        "sass-loader": "^10.1.1",
        "style-loader": "^2.0.0",
        "svg-url-loader": "^7.1.1",
        "webpack": "^5.18.0",
        "webpack-cli": "^4.4.0"
    },
    "config": {
        "forge": {
            "packagerConfig": {
                "icon": "src/favicon.ico"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "everyone.dance"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin",
                        "linux"
                    ]
                }
            ]
        }
    }
}
